{
    "name": "xphere/tag-bundle",
    "description": "Helps collecting tagged services to inject them into others",
    "license": "MIT",
    "homepage": "https://github.com/xphere/tag-bundle",
    "authors": [
        {
            "name": "Berny Cantos",
            "email": "be@rny.cc"
        }
    ],
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Xphere\\Tag\\": "src",
            "Xphere\\Bundle\\TagBundle\\": ""
        }
    },
    "require": {
        "symfony/dependency-injection": ">=2.0.10",
        "symfony/http-kernel": ">=2.0.10",
        "symfony/config": ">=2.0.10"
    },
    "require-dev": {
        "php": ">=5.5",
        "phpunit/phpunit": "^4.5"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
